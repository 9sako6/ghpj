{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2016
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:@typescript-eslint/eslint-recommended"
  ],
  "env": {
    "es6": true
  },
  "rules": {
    "eol-last": "error",
    "semi": "error",
    "ImportDeclaration": "off"
  }
}
